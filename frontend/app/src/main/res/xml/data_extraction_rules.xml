<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include shared preferences (e.g., TokenManager data) -->
    <include domain="sharedpref" path="InventoryEasePrefs.xml"/>

    <!-- Exclude temporary files or caches -->
    <exclude domain="file" path="cache/"/>
    <exclude domain="file" path="temp/"/>

    <!-- Exclude sensitive data (e.g., database files with user info) -->
    <exclude domain="database" path="user_data.db"/>
</full-backup-content>